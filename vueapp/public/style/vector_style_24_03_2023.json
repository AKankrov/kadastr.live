{
  "version": 8,
  "name": "cadaster",
  "metadata": {"maputnik:renderer": "mbgljs"},
  "sources": {
    "cadastr": {
      "type": "vector",
      "url": "https://kadastr.live/tiles/capabilities/kadastr.json?version=2023-03-24",
      "attribution": "<a target=\"_blank\" href=\"https://github.com/Monstrofil/kadastr.live/\"><img style=\"vertical-align: middle;\" src=\"/static/GitHub_Logo.png\"></a>"
    },
    "dzk_pzf": {
      "type": "vector",
      "url": "https://kadastr.live/tiles/capabilities/dzk_pzf.json?version=2023-03-24",
      "attribution": "<a target=\"_blank\" href=\"https://github.com/Monstrofil/kadastr.live/\"><img style=\"vertical-align: middle;\" src=\"/static/GitHub_Logo.png\"></a>"
    },
    "dzk_atu": {
      "type": "vector",
      "url": "https://kadastr.live/tiles/capabilities/dzk_atu.json?version=2023-03-24",
      "attribution": "DZK"
    },
    "dzk_index_map": {
      "type": "vector",
      "url": "https://kadastr.live/tiles/capabilities/dzk_index_map.json?version=2023-03-24",
      "attribution": "<a target=\"_blank\" href=\"https://github.com/Monstrofil/kadastr.live/\"><img style=\"vertical-align: middle;\" src=\"/static/GitHub_Logo.png\"></a>"
    },
    "raster-tiles": {
      "type": "raster",
      "tiles": [
        "https://a.tile.openstreetmap.org/{z}/{x}/{y}.png",
        "https://b.tile.openstreetmap.org/{z}/{x}/{y}.png",
        "https://c.tile.openstreetmap.org/{z}/{x}/{y}.png"
      ],
      "tileSize": 256,
      "attribution": "OpenStreetMap contributors",
      "maxzoom": 19
    },
    "arcgis-world-imagery-ZYX": {
      "type": "raster",
      "tiles": [
        "https://services.arcgisonline.com/arcgis/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
      ],
      "tileSize": 256,
      "attribution": "Esri, Maxar, GeoEye, Earthstar Geographics, CNES/Airbus DS, USDA, USGS, AeroGRID, IGN, and the GIS User Community",
      "maxzoom": 17
    },
    "orto-tiles": {
      "type": "raster",
      "tiles": [
        "https://data.gki.com.ua/api-user/rtile/orto10/ua/{z}/{x}/{y}.png"
      ],
      "tileSize": 256,
      "attribution": "DZK",
      "maxzoom": 19
    },
    "topo-tiles": {
      "type": "raster",
      "tiles": [
        "https://data.gki.com.ua/api-user/rtile/topo100/ua/{z}/{x}/{y}.png"
      ],
      "tileSize": 256,
      "attribution": "ДНІГК",
      "maxzoom": 19
    },
    "wms-kadastr-source": {
      "type": "raster",
      "tiles": [
        "https://m1.land.gov.ua/geowebcache/service/wms?bbox={bbox-epsg-3857}&format=image/png&service=WMS&version=1.1.1&request=GetMap&srs=EPSG:3857&transparent=true&width=256&height=256&layers=kadastr"
      ],
      "tileSize": 256,
      "attribution": "ДЗК",
      "maxzoom": 16
    },
    "dummy": {
      "type": "geojson",
      "data": {"type": "FeatureCollection", "features": []}
    },
    "dzk_water_map": {
      "type": "vector",
      "url": "https://kadastr.live/tiles/capabilities/dzk_water_map.json"
    }
  },
  "glyphs": "https://fonts.openmaptiles.org/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "openstreetmap",
      "type": "raster",
      "source": "raster-tiles",
      "minzoom": 0,
      "maxzoom": 22,
      "layout": {"visibility": "visible"}
    },
    {
      "id": "orto-ersi",
      "type": "raster",
      "source": "arcgis-world-imagery-ZYX",
      "minzoom": 0,
      "maxzoom": 22,
      "layout": {"visibility": "none"}
    },
    {
      "id": "orto-tiles",
      "type": "raster",
      "source": "orto-tiles",
      "minzoom": 0,
      "maxzoom": 22,
      "layout": {"visibility": "none"}
    },
    {
      "id": "topo-tiles",
      "type": "raster",
      "source": "topo-tiles",
      "minzoom": 0,
      "maxzoom": 22,
      "layout": {"visibility": "none"}
    },
    {
      "id": "dzk__atu_oblast",
      "type": "line",
      "source": "dzk_atu",
      "source-layer": "atu_oblast_data",
      "layout": {"visibility": "none"},
      "paint": {
        "line-color": "rgba(0, 30, 92, 0.5)",
        "line-width": 3
      }
    },
    {
      "id": "dzk__index_map_lines",
      "type": "line",
      "source": "dzk_index_map",
      "source-layer": "index_data",
      "minzoom": 0,
      "layout": {
        "visibility": "none",
        "line-cap": "butt",
        "line-join": "bevel",
        "line-round-limit": 2
      },
      "paint": {
        "line-color": "rgba(0, 40, 251,  0.7)",
        "line-gap-width": 0,
        "line-blur": 2,
        "line-width": {"stops": [[10, 0.5], [13, 2], [16, 4]]}
      }
    },
    {
      "id": "dzk__index_map_poly",
      "type": "fill",
      "source": "dzk_index_map",
      "source-layer": "index_data",
      "layout": {"visibility": "none"},
      "paint": {"fill-color": "rgba(0, 193, 255, 1)", "fill-opacity": 0}
    },
    {
      "id": "dzk",
      "type": "raster",
      "source": "wms-kadastr-source",
      "minzoom": 0,
      "maxzoom": 22,
      "layout": {"visibility": "none"}
    },
    {
      "id": "land_polygones",
      "type": "fill",
      "source": "cadastr",
      "source-layer": "land_polygons",
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-opacity": [
          "case",
          ["boolean", ["feature-state", "hover"], false],
          ["literal", 0.85],
          ["literal", 0.5]
        ],
        "fill-color": [
          "case",
          ["boolean", ["==", ["get", "address"], null], false],
          "rgba(41, 162, 200, 1)",
          "rgba(41, 200, 162, 1)"
        ],
        "fill-outline-color": [
          "case",
          ["boolean", ["==", ["get", "address"], null], false],
          "rgba(0, 0, 0, 0.6)",
          "rgba(0, 0, 0, 0.6)"
        ]
      }
    },
    {
      "id": "landuse_count_points",
      "type": "symbol",
      "source": "cadastr",
      "source-layer": "centroids",
      "maxzoom": 7,
      "layout": {
        "text-field": "{parcels}",
        "text-font": ["Open Sans Bold"],
        "text-ignore-placement": false,
        "text-allow-overlap": false,
        "text-anchor": "center",
        "text-justify": "center",
        "text-letter-spacing": 0,
        "text-keep-upright": false,
        "text-transform": "none",
        "text-optional": false,
        "text-pitch-alignment": "auto",
        "text-size": {"stops": [[6, 18], [7, 21]]}
      },
      "paint": {
        "text-color": "rgba(0, 0, 0, 1)",
        "text-halo-color": "rgba(255, 255, 255, 1)",
        "text-halo-width": 2
      }
    },
    {
      "id": "landuse_count_background",
      "type": "fill",
      "source": "cadastr",
      "source-layer": "polygons",
      "maxzoom": 8,
      "paint": {
        "fill-color": {
          "stops": [
            [3, "rgba(255, 255, 255, 0.5)"],
            [5, "rgba(255, 255, 255, 0.3)"],
            [8, "rgba(0, 0, 0, 0)"]
          ]
        }
      }
    },
    {
      "id": "dzk__pzf",
      "type": "fill",
      "source": "dzk_pzf",
      "source-layer": "pzf_data",
      "layout": {"visibility": "none"},
      "paint": {
        "fill-color": "rgba(30, 92, 0, 0.5)",
        "fill-antialias": true,
        "fill-outline-color": "rgba(32, 32, 32, 0.5)"
      }
    },
    {
      "id": "dzk__atu_rayon",
      "type": "line",
      "source": "dzk_atu",
      "source-layer": "atu_rayon_data",
      "layout": {"visibility": "none"},
      "paint": {
        "line-color": "rgba(0, 0, 90, 0.5)",
        "line-width": 2
      }
    },
    {
      "id": "dzk__atu_rayon__text",
      "type": "symbol",
      "source": "dzk_atu",
      "source-layer": "atu_rayon_data",
      "layout": {
        "text-field": "{ADMIN_2}",
        "visibility": "none",
        "symbol-placement": "line",
        "symbol-spacing": 250,
        "text-font": [
          "Klokantech Noto Sans Bold"
        ],
        "symbol-avoid-edges": true,
        "text-allow-overlap": true,
        "text-ignore-placement": false,
        "text-padding": 0,
        "text-pitch-alignment": "auto",
        "text-rotation-alignment": "auto",
        "text-offset": [0, 1],
        "icon-allow-overlap": false,
        "icon-ignore-placement": false,
        "text-size": 14,
        "text-keep-upright": true
      },
      "paint": {
        "text-color": "rgba(0, 0, 90, 0.9)",
        "text-halo-color": "rgba(255, 255, 255, 0.5)",
        "text-halo-width": 1
      }
    },
    {
      "id": "dzk__atu_terhromad__line",
      "type": "line",
      "source": "dzk_atu",
      "source-layer": "atu_terhromad_data",
      "layout": {"visibility": "none"},
      "paint": {
        "line-color": "rgba(180, 30, 0, 0.5)",
        "line-width": 2,
        "line-dasharray": ["literal", [2, 2]]
      }
    },
    {
      "id": "dzk__atu_terhromad",
      "type": "fill",
      "source": "dzk_atu",
      "source-layer": "atu_terhromad_data",
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(92, 30, 0, 0)"
      }
    },
    {
      "id": "dzk__atu_terhromad__text",
      "type": "symbol",
      "source": "dzk_atu",
      "source-layer": "atu_terhromad_data",
      "layout": {
        "text-field": "{ADMIN_3} {TYPE}",
        "visibility": "none",
        "symbol-placement": "line",
        "symbol-spacing": 250,
        "text-font": [
          "Klokantech Noto Sans Bold"
        ],
        "symbol-avoid-edges": true,
        "text-allow-overlap": true,
        "text-ignore-placement": false,
        "text-padding": 0,
        "text-pitch-alignment": "auto",
        "text-rotation-alignment": "auto",
        "text-offset": [0, 1],
        "icon-allow-overlap": false,
        "icon-ignore-placement": false,
        "text-size": 14,
        "text-keep-upright": true
      },
      "paint": {
        "text-color": "rgba(150, 19, 19, 0.9)",
        "text-halo-color": "rgba(255, 255, 255, 0.5)",
        "text-halo-width": 1
      }
    },
    {
      "id": "water_lines_text",
      "type": "symbol",
      "source": "dzk_water_map",
      "source-layer": "river_line",
      "layout": {
        "text-field": "{name_ua}",
        "visibility": "visible",
        "symbol-placement": "line-center",
        "text-font": ["Open Sans Bold"],
        "symbol-avoid-edges": false,
        "text-offset": [0, 1],
        "text-allow-overlap": false,
        "text-pitch-alignment": "auto"
      },
      "paint": {
        "text-color": "rgba(45, 79, 255, 1)",
        "text-halo-color": "rgba(255, 255, 255, 1)",
        "text-halo-width": 2,
        "text-halo-blur": 1
      }
    },
    {
      "id": "water_lines_other",
      "type": "line",
      "source": "dzk_water_map",
      "source-layer": "river_line",
      "filter": ["all", ["<", "length", 50]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(45, 79, 255, 1)",
        "line-width": {"stops": [[5, 0.5], [12, 2]]},
        "line-blur": 1.5
      }
    },
    {
      "id": "water_lines_middle_rivers",
      "type": "line",
      "source": "dzk_water_map",
      "source-layer": "river_line",
      "filter": ["all", [">=", "length", 50]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(45, 79, 255, 1)",
        "line-width": {"stops": [[5, 1], [12, 4]]},
        "line-blur": 1.5
      }
    },
    {
      "id": "water_lines_large",
      "type": "line",
      "source": "dzk_water_map",
      "source-layer": "river_line",
      "filter": ["all", [">=", "length", 100]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(45, 79, 255, 1)",
        "line-width": {"stops": [[5, 2], [12, 6]]},
        "line-blur": 2
      }
    }
  ],
  "id": "positron"
}